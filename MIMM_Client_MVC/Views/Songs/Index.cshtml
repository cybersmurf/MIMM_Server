@model IEnumerable<MIMM_Shared.Models.Songs>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Whispertime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Idfeeling)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdtracksNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Whispertime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Idfeeling)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdtracksNavigation.Name)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
