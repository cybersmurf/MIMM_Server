@model MIMM_Shared.Models.Songs

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Create</h1>

<h4>Songs</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Idtracks" class="control-label"></label>
                <select asp-for="Idtracks" class ="form-control" asp-items="ViewBag.Idtracks"></select>
            </div>
            <div class="form-group">
                <label asp-for="Idfeeling" class="control-label"></label>
                <input asp-for="Idfeeling" class="form-control" />
                <span asp-validation-for="Idfeeling" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
